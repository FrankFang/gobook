{
  "prettier.enable": false,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.quiet": true,
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "html",
    "markdown"
  ],
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.fixAll.eslint": true,
    "source.organizeImports": false
  },
  "cSpell.words": [
    "Attributify",
    "gobook",
    "immer",
    "Klass",
    "unocss",
    "wailsjs",
    "windicss",
    "zustand"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": false,
    "**/Thumbs.db": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.history": true,
    "**/.project": true,
    "**/.settings": true,
    "**/*.acn": true,
    "**/*.acr": true,
    "**/*.alg": true,
    "**/*.aux": true,
    "**/*.bcf": true,
    "**/*.glg2": true,
    "**/*.glo2": true,
    "**/*.gls2": true,
    "**/*.ist": true,
    "**/*.log": true,
    "**/*.run.xml": true,
    "**/node_modules": true,
    "**/dist": true
  },
  "explorerExclude.backup": {
  }
}
